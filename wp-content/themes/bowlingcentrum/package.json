    "parcel": "^2.9.3",
    "parcel-plugin-watch-reload": "^1.0.3",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1"
  },
  "watchreload": {
    "files": [
      "**/*.php",
      "**/**/*.php",
      "**/**/*.svg"
    ]
